{
  "name": "bizzi-i18n-vscode-extension",
  "displayName": "bizzi-i18n-vscode-extension",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:bizzi-i18n-vscode-extension.extractI18NFromSelected",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bizzi-i18n-vscode-extension.extractI18NFromSelected",
        "title": "I18n: Extract from selected text"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/parser": "^7.19.3",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/traverse": "^7.19.3",
    "@babel/types": "^7.19.3",
    "@types/lodash": "^4.14.186",
    "async-await-retry": "^2.0.0",
    "lodash": "^4.17.21",
    "vscode-uri": "^3.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hungphongbk/bizzi-i18n-vscode-extension.git"
  },
  "publisher": "phongtruonghung"
}
